standby_controller_addr: {{ item.0.standby_controller_ip }}
{% if vrs_arch|string() == "el6" %}
vrs_package_path: {{ el6_vrs_package_path }}
vrs_package_file_name_list:
{% for el6_file_name in el6_vrs_package_file_name_list %}
