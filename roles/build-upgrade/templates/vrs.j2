{% for u14_04_file_name in u14_04_vrs_package_file_name_list %}
  - {{ u14_04_file_name }}
{% endfor %}
{% elif vrs_arch|string() == "u16.04" %}
vrs_package_path: {{ u16_04_vrs_package_path }}
vrs_package_file_name_list:
{% for u16_04_file_name in u16_04_vrs_package_file_name_list %}
  - {{ u16_04_file_name }}
{% endfor %}
{% endif %}
{% set dockermon_install = dockermon_install | default(false) %}
{% if dockermon_install == true or dockermon_install == True %}
{% if vrs_arch|string() == "el6" or vrs_arch|string() == "el7" %}
dockermon_package_path: {{ dockermon_package_path }}
dockermon_package_file_name: {{ dockermon_package_file_name }}
{% elif vrs_arch|string() == "u14.04"%}
dockermon_package_path: {{ u14_04_dockermon_package_path }}
dockermon_package_file_name: {{ u14_04_dockermon_package_file_name }}
{% elif vrs_arch|string() == "u16.04"%}
dockermon_package_path: {{ u16_04_dockermon_package_path }}
dockermon_package_file_name: {{ u16_04_dockermon_package_file_name }}
{% endif %}
dockermon_install: true
{% endif %}
