{% endfor %}
{% elif vrs_arch|string() == "el7" %}
vrs_package_path: {{ el7_vrs_package_path }}
vrs_package_file_name_list:
{% for el7_file_name in el7_vrs_package_file_name_list %}
  - {{ el7_file_name }}
{% endfor %}
{% elif vrs_arch|string() == "u14.04" %}
vrs_package_path: {{ u14_04_vrs_package_path }}
