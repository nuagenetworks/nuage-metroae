target_server_type: {{ item.target_server_type }}
{% if item.target_server_type == "kvm" or item.target_server_type == "vcenter" %}
target_server: {{ item.target_server }}

interfaces:
  mgmt:
    ip: {{ item.mgmt_ip }}
    gateway: {{ item.mgmt_gateway }}
    netmask: {{ item.mgmt_netmask }}

vsd_fqdn: {{ item.vsd_fqdn }}

{% if item.target_server_type == "kvm" %}
vstat_qcow2_path: {{ vstat_qcow2_path }}
vstat_qcow2_file_name: {{ vstat_qcow2_file_name }}
{% endif %}
{% if item.target_server_type == "vcenter" %}
vstat_ovf_path: {{ vstat_ovf_path }}
vstat_ovf_file_name: {{ vstat_ovf_file_name }}
{% endif %}
{% endif %}
