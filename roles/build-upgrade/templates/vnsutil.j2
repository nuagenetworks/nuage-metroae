target_server_type: {{ item.target_server_type }}
{% if item.target_server_type == "kvm" %}
target_server: {{ item.target_server }}
hostname: {{ item.hostname }}
{% if item.vmname is defined %}
vmname: {{ item.vmname }}
{% else %}
vmname: {{ item.hostname }}
{% endif %}
interfaces:
  mgmt:
    ip: {{ item.mgmt_ip }}
    gateway: {{ item.mgmt_gateway }}
    netmask: {{ item.mgmt_netmask }}
  data:
    ip: {{ item.data_ip }}
    netmask: {{ item.data_netmask }}
    subnet: {{ item.data_subnet }}
  nsgv:
    ip: {{ item.nsgv_ip }}
    mac: '{{ item.nsgv_mac }}'
    host_name: {{ item.nsgv_hostname }}

vsd_fqdn: {{ item.vsd_fqdn }}

vnsutil_qcow2_path: {{ vnsutil_qcow2_path }}
vnsutil_qcow2_file_name: {{ vnsutil_qcow2_file_name }}

{% endif %}
