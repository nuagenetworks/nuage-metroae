{% set vrs_arch = item.0.vrs_os_type %}
vrs_arch: {{ vrs_arch }}
node_ip_addr: {{ item.1 }}
active_controller_addr: {{ item.0.active_controller_ip }}
standby_controller_addr: {{ item.0.standby_controller_ip }}
vrs_package_path: {{ vrs_package_path }}
{% if vrs_arch|string() == "Redhat" %}
vrs_package_file_name_list:
{% for file_name in vrs_package_file_name_list %}
  - {{ file_name }}
{% endfor %}
{% elif vrs_arch|string() == "Debian" %}
vrs_package_file_name_list_deb:
{% for file_name2 in vrs_package_file_name_list_deb %}
  - {{ file_name2 }}
{% endfor %}
{% endif %}
{% if dockermon_install == true or dockermon_install == True %}
dockermon_package_path: {{ dockermon_package_path }}
dockermon_package_file_name: {{ dockermon_package_file_name }}
dockermon_install: true
{% endif %}
