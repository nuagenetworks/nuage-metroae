---
# *** WARNING ***
# This file is automatically generated by build.yml.
# Changes made to this file may be overwritten.
#
mgmt_bridge: {{ mgmt_bridge | default('NONE') }}
data_bridge: {{ data_bridge | default('NONE') }}
access_bridge: {{ access_bridge | default('NONE') }}
images_path: {{ images_path | default('NONE') }}
ansible_deployment_host: {{ ansible_deployment_host }}
ntp_server_list:
{% for server in ntp_server_list %}
  - {{ server }}
{% endfor %}
dns_server_list:
{% for server in dns_server_list %}
  - {{ server }}
{% endfor %}
dns_domain: {{ dns_domain }}
timezone: {{ timezone | default('US/Pacific') }}

{% if os_auth is defined %}
os_auth:
  username: {{ os_auth.username | default('NONE') }}
  password: {{ os_auth.password | default('NONE') }}
  project_name: {{ os_auth.project_name | default('NONE') }}
  auth_url: "{{ os_auth.auth_url | default('NONE') }}"

{% endif %}
{% if vcenter is defined %}
vcenter:
  username: {{ vcenter.username | default('NONE') }}
  password: {{ vcenter.password | default('NONE') }}
  datacenter: {{ vcenter.datacenter | default('NONE') }}
  cluster: {{ vcenter.cluster | default('NONE') }}
  datastore: {{ vcenter.datastore | default('NONE') }}
  ovftool: {{ vcenter.ovftool | default('NONE') }}

{% endif %}
