---
nuage_zipped_files_dir: "/mnt/nfs/nuage-releases/VERSION/nuage-packed"
nuage_unzipped_files_dir: "/mnt/nfs/nuage-releases/VERSION/nuage-unpacked"
target_server_username: "root"
ansible_sudo_username: "root"
vsd_fqdn_global: vsd1.example.com
vsd_operations_list:
  - config
myvsds:
  - { hostname: vsd1.example.com,
      target_server_type: "kvm",
      target_server: 135.227.181.233,
      mgmt_ip: 192.168.122.211,
      mgmt_gateway: 192.168.122.2,
      mgmt_netmask: 255.255.255.0 }
vns_operations_list:
  - install
myvnsutils:
  - { hostname: vnsutil1.example.com,
      target_server_type: "kvm",
      target_server: 135.227.181.233,
      mgmt_ip: 192.168.122.215,
      mgmt_gateway: 135.227.181.233,
      mgmt_netmask: 255.255.255.0,
      data_fqdn: vnsutil1.data.example.com,
      data_ip: 192.168.100.215,
      data_subnet: 192.168.100.0,
      data_netmask: 255.255.255.0,
      nsgv_ip: 192.168.100.216,
      nsgv_mac: '52:54:00:88:85:12',
      nsgv_hostname: nsgv1.example.com,
      vsd_fqdn: vsd1.example.com }
mynsgvs:
  - { hostname: nsgv1.example.com,
      target_server_type: "kvm",
      target_server: 135.227.181.233,
      bootstrap_method: zfb_metro,
      nsgv_mac: '52:54:00:88:85:12' }
mgmt_bridge: "mgmt"
data_bridge: "data"
access_bridge: "access"
images_path: "/var/lib/libvirt/images/"
ntp_server_list:
  - 135.227.181.233
  - 192.96.202.120
dns_server_list:
  - 135.227.181.233
  - 127.0.0.1
dns_domain: example.com
