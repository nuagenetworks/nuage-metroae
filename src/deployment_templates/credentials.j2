    vstat_custom_username: "{{ item.vstat_custom_username }}"
    {%- else %}
    # vstat_custom_username: ""
    {%- endif %}

    # < ElasticSearch (Stats) System Password >
    # ElasticSearch (Stats) password to login into command line
    #
    {%- if item.vstat_custom_password is defined %}
    vstat_custom_password: {{ item.vstat_custom_password | indent(8, False) }}
    {%- else %}
    # vstat_custom_password: ""
    {%- endif %}

