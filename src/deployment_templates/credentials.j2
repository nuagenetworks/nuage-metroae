###############################################################################
# Credentials
#
# Nuage Platform and Metro√Ü Credentials.  In the current version, only the first credentials set will be utilized.
#
# Automatically generated by {{ generator_script | default("script") }}.
#

{% if credentials is defined and credentials %}
{% for item in credentials %}
#
# Set {{ loop.index }}
#
-
    # < Name >
    # Name of credentials set.  In the current version, only the first credentials set will be utilized.
    #
    name: "{{ item.name }}"

    {%- if show_target_server_type | default('kvm') == 'kvm' %}

    # < Target server username >
    # Remote user name for ansible to execute as on the target server (hypervisor).
    #
    {%- if item.target_server_username is defined %}
    target_server_username: "{{ item.target_server_username }}"
    {%- else %}
    # target_server_username: root
    {%- endif %}
    {%- endif %}

    # < Target server password >
    # Remote password for target server (hypervisor) to for TLS secure VRS communication.
    #
    {%- if item.target_server_password is defined %}
    target_server_password: {{ item.target_server_password | indent(8, False) }}
    {%- else %}
    # target_server_password: ""
    {%- endif %}

    {%- if show_target_server_type | default('vcenter') == 'vcenter' %}

    # < vCenter Username >
    # vCenter Username
    #
    {%- if item.vcenter_username is defined %}
    vcenter_username: "{{ item.vcenter_username }}"
    {%- else %}
    # vcenter_username: ""
    {%- endif %}
    {%- endif %}

    {%- if show_target_server_type | default('vcenter') == 'vcenter' %}

    # < vCenter Password >
    # vCenter Password
    #
    {%- if item.vcenter_password is defined %}
    vcenter_password: {{ item.vcenter_password | indent(8, False) }}
    {%- else %}
    # vcenter_password: ""
    {%- endif %}
    {%- endif %}

    # < VSD API/Architect username >
    # Username for API authentication. Must have csproot privileges. Also known as csproot user
    #
    {%- if item.vsd_auth_username is defined %}
    vsd_auth_username: "{{ item.vsd_auth_username }}"
    {%- else %}
    # vsd_auth_username: csproot
    {%- endif %}

    # < VSD API/Architect Password >
    # Password for API authentication. Must have csproot privileges. Also known as csproot password
    #
    {%- if item.vsd_auth_password is defined %}
    vsd_auth_password: {{ item.vsd_auth_password | indent(8, False) }}
    {%- else %}
    # vsd_auth_password: csproot
    {%- endif %}

    # < VSD System Username >
    # Username to be used while logging into VSD command line.
    #
    {%- if item.vsd_custom_username is defined %}
    vsd_custom_username: "{{ item.vsd_custom_username }}"
    {%- else %}
    # vsd_custom_username: root
    {%- endif %}

    # < VSD System Password >
    # Password for the VSD user used to login to the command line
    #
    {%- if item.vsd_custom_password is defined %}
    vsd_custom_password: {{ item.vsd_custom_password | indent(8, False) }}
    {%- else %}
    # vsd_custom_password: Alcateldc
    {%- endif %}

    # < VSC System Username >
    # VSC username to login into command line. Should have admin privileges.
    #
    {%- if item.vsc_custom_username is defined %}
    vsc_custom_username: "{{ item.vsc_custom_username }}"
    {%- else %}
    # vsc_custom_username: ""
    {%- endif %}

    # < VSC System Password >
    # VSC password to login into command line
    #
    {%- if item.vsc_custom_password is defined %}
    vsc_custom_password: {{ item.vsc_custom_password | indent(8, False) }}
    {%- else %}
    # vsc_custom_password: ""
    {%- endif %}

    # < ElasticSearch (Stats) System Username >
