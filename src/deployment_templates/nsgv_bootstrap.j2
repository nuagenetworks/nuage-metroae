###############################################################################
# NSGv Bootstrap
#
# Specify zero-factor bootstrap configuration for NSGvs.
#
# Automatically generated by {{ generator_script | default("script") }}.
#

##### Organization and Proxy User

# < Global organization for NSGvs >
# Global organization name for NSGvs on the VSD.
#
{%- if nsgv_organization is defined %}
nsgv_organization: "{{ nsgv_organization }}"
{%- else %}
# nsgv_organization: ""
{%- endif %}

# < Proxy User First Name >
# First name for proxy user. Password for proxy user is set in credentials.
#
{%- if proxy_user_first_name is defined %}
proxy_user_first_name: "{{ proxy_user_first_name }}"
{%- else %}
# proxy_user_first_name: ""
{%- endif %}

# < Proxy User Last Name >
# Last name for proxy user. Password for proxy user is set in credentials.
#
{%- if proxy_user_last_name is defined %}
proxy_user_last_name: "{{ proxy_user_last_name }}"
{%- else %}
# proxy_user_last_name: ""
{%- endif %}

# < Proxy User Email >
# Email address for proxy user. Password for proxy user is set in credentials.
#
{%- if proxy_user_email is defined %}
proxy_user_email: "{{ proxy_user_email }}"
{%- else %}
# proxy_user_email: ""
{%- endif %}

#################################

##### NSG Infrastructure

# < NSG Infrastructure Profile Name >
# Name for the infrastructure gateway profile on the VSD.
#
{%- if nsg_infra_profile_name is defined %}
nsg_infra_profile_name: "{{ nsg_infra_profile_name }}"
{%- else %}
# nsg_infra_profile_name: ""
{%- endif %}

# < NSGv Template Name >
# Name for the gateway template on the VSD.
#
{%- if nsg_template_name is defined %}
nsg_template_name: "{{ nsg_template_name }}"
{%- else %}
# nsg_template_name: ""
{%- endif %}

# < Proxy DNS Name >
# DNS name for the proxy.
#
{%- if proxy_dns_name is defined %}
proxy_dns_name: "{{ proxy_dns_name }}"
{%- else %}
# proxy_dns_name: ""
{%- endif %}

# < Allow SSH Access Override >
# Allows NSGs using this profile to override SSH access.
#
{%- if ssh_override is defined %}
ssh_override: {{ ssh_override | lower }}
{%- else %}
# ssh_override: False
{%- endif %}

########################

##### VSC Infrastructure

# < VSC Infrastructure Profile Name >
# Name of the infrastructure VSC profile on the VSD.
#
{%- if vsc_infra_profile_name is defined %}
vsc_infra_profile_name: "{{ vsc_infra_profile_name }}"
{%- else %}
# vsc_infra_profile_name: ""
{%- endif %}

# < Address of First VSC >
# IP address of the first controller.
#
{%- if first_controller_address is defined %}
first_controller_address: "{{ first_controller_address }}"
{%- else %}
# first_controller_address: ""
{%- endif %}

# < Address of Second VSC >
# IP address of the second controller.
#
{%- if second_controller_address is defined %}
second_controller_address: "{{ second_controller_address }}"
{%- else %}
# second_controller_address: ""
{%- endif %}

